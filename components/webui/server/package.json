{
  "name": "@webui/server",
  "version": "0.1.0",
  "description": "",
  "main": "src/main.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "npm run build -- -w --preserveWatchOutput",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/src/app.js",
    "dev": "npm run build && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:build:watch\" \"npm:dev:start\"",
    "start": "npm run build && fastify start -l info dist/src/app.js",
    "standalone": "npm run build && node --env-file=.env dist/src/main.js",
    "lint:check": "eslint . --max-warnings 0",
    "lint:fix": "npm run lint:check -- --fix",
    "test": "tap --include='**/*test.ts'"
  },
  "author": "YScope Inc. <dev@yscope.com>",
  "license": "Apache-2.0",
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.758.0",
    "@aws-sdk/s3-request-presigner": "^3.758.0",
    "@fastify/autoload": "^6.3.0",
    "@fastify/env": "^5.0.2",
    "@fastify/http-proxy": "^11.3.0",
    "@fastify/mongodb": "^9.0.2",
    "@fastify/mysql": "^5.0.2",
    "@fastify/rate-limit": "^10.2.2",
    "@fastify/sensible": "^6.0.3",
    "@fastify/static": "^8.1.1",
    "@fastify/type-provider-typebox": "^5.1.0",
    "@msgpack/msgpack": "^3.1.0",
    "@sinclair/typebox": "^0.34.38",
    "@types/node": "^22.13.9",
    "@webui/common": "*",
    "close-with-grace": "^2.2.0",
    "dotenv": "^16.4.7",
    "fastify": "^5.4.0",
    "fastify-plugin": "^5.0.1",
    "http-status-codes": "^2.3.0",
    "pino-pretty": "^13.0.0",
    "presto-client": "^1.1.0",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/presto-client": "^1.0.2",
    "concurrently": "^9.2.1",
    "eslint-config-yscope": "1.1.1",
    "fastify-cli": "^7.4.0",
    "tap": "^21.1.0",
    "typescript": "^5.7.3"
  }
}
